{
    "name": "nadar/php-composer-reader",
    "description": "Read and manipulate composer.json",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^3.15",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.15.23"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Basil Suter",
            "email": "git@nadar.io"
        }
    ],
    "require": {
        "php": ">=8",
        "composer/semver": "^3"
    },
    "autoload": {
        "psr-4": {
            "Nadar\\PhpComposerReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nadar\\PhpComposerReader\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan -v",
        "phpcsfixer": "vendor/bin/php-cs-fixer fix",
        "rector": "vendor/bin/rector"
    }
}